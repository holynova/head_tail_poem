[{"line":"骑牛远远过前村，短笛横吹隔陇闻","poemId":"poem_128"},{"line":"岐王宅里寻常见，崔九堂前几度闻","poemId":"poem_326"},{"line":"余亦能高咏，斯人不可闻","poemId":"poem_376"},{"line":"吾爱孟夫子，风流天下闻","poemId":"poem_377"},{"line":"唯见林花落，莺啼送客闻","poemId":"poem_394"},{"line":"骊宫高处入青云，仙乐风飘处处闻","poemId":"poem_526"},{"line":"贺兰山下阵如云，羽檄交驰日夕闻","poemId":"poem_561"},{"line":"华亭鹤唳讵可闻","poemId":"poem_573"},{"line":" 茅亭宿花影，药院滋苔纹","poemId":"poem_522"},{"line":"试拂铁衣如雪色，聊持宝剑动星文","poemId":"poem_561"}]