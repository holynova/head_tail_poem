[{"line":"七兮","poemId":"poem_7"},{"line":"不如子之衣，安且吉兮","poemId":"poem_7"},{"line":"六兮","poemId":"poem_7"},{"line":"不如子之衣，安且燠兮","poemId":"poem_7"},{"line":"坎坎伐檀兮，置之河之干兮","poemId":"poem_12"},{"line":"不稼不穑，胡取禾三百廛兮","poemId":"poem_12"},{"line":"不狩不猎，胡瞻尔庭有县貆兮","poemId":"poem_12"},{"line":"彼君子兮，不素餐兮","poemId":"poem_12"},{"line":"坎坎伐辐兮，置之河之侧兮","poemId":"poem_12"},{"line":"不稼不穑，胡取禾三百亿兮","poemId":"poem_12"},{"line":"不狩不猎，胡瞻尔庭有县特兮","poemId":"poem_12"},{"line":"彼君子兮，不素食兮","poemId":"poem_12"},{"line":"坎坎伐轮兮，置之河之漘兮","poemId":"poem_12"},{"line":"不稼不穑，胡取禾三百囷兮","poemId":"poem_12"},{"line":"不狩不猎，胡瞻尔庭有县鹑兮","poemId":"poem_12"},{"line":"彼君子兮，不素飧兮","poemId":"poem_12"}]