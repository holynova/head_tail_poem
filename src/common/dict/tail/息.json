[{"line":"日出而作，日入而息","poemId":"poem_4"},{"line":"对案不能食，拔剑击柱长叹息","poemId":"poem_53"},{"line":"弃置罢官去，还家自休息","poemId":"poem_53"},{"line":"零泪向谁道，鸡鸣徒叹息","poemId":"poem_62"},{"line":"莫作瓶落井，一去无消息","poemId":"poem_64"},{"line":"江南瘴疠地，逐客无消息","poemId":"poem_501"},{"line":"江南瘴疠地，逐客无消息","poemId":"poem_501"},{"line":"郑国游人未及家，洛阳行子空叹息","poemId":"poem_543"},{"line":"清渭东流剑阁深，去住彼此无消息","poemId":"poem_587"}]