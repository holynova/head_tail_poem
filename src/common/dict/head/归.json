[{"line":"归来涧底磨刀斧，又作全家明日谋","poemId":"poem_155"},{"line":"归路畏逢虎，况闻岩下风","poemId":"poem_253"},{"line":"归山深浅去，须尽丘壑美","poemId":"poem_302"},{"line":"归雁洛阳边","poemId":"poem_367"},{"line":"归来视幼女，零泪缘缨流","poemId":"poem_515"},{"line":"归棹洛阳人，残钟广陵树","poemId":"poem_520"},{"line":"归来池苑皆依旧，太液芙蓉未央柳","poemId":"poem_526"}]