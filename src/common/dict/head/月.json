[{"line":"月明星稀，乌鹊南飞","poemId":"poem_32"},{"line":"月明船笛参差起，风定池莲自在香","poemId":"poem_133"},{"line":"月儿弯弯照九州，几家欢乐几家愁","poemId":"poem_202"},{"line":"月黑见渔灯，孤光一点萤","poemId":"poem_248"},{"line":"月在众峰顶，泉流乱叶中","poemId":"poem_253"},{"line":"月落乌啼霜满天，江枫渔火对愁眠","poemId":"poem_350"},{"line":"月下飞天镜，云生结海楼","poemId":"poem_378"},{"line":"月既不解饮，影徒随我身","poemId":"poem_498"},{"line":"月照城头乌半飞，霜凄万树风入衣","poemId":"poem_539"},{"line":"月明松下房栊静，日出云中鸡犬喧","poemId":"poem_564"},{"line":"月黑雁飞高，单于夜遁逃","poemId":"poem_596"}]