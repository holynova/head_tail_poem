[{"line":"维常之华","poemId":"poem_1"},{"line":"唯有杜康","poemId":"poem_32"},{"line":"唯有南风旧相识，偷开门户又翻书","poemId":"poem_117"},{"line":"唯见林花落，莺啼送客闻","poemId":"poem_394"},{"line":"惟怜一灯影，万里眼中明","poemId":"poem_433"},{"line":"惟将终夜常开眼，报答平生未展眉","poemId":"poem_439"},{"line":"惟将迟暮供多病，未有涓埃答圣朝","poemId":"poem_468"},{"line":"惟将旧物表深情，钿合金钗寄将去","poemId":"poem_526"},{"line":"惟觉时之枕席，失向来之烟霞","poemId":"poem_534"}]