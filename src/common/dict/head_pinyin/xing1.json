[{"line":"星汉灿烂，若出其里","poemId":"poem_31"},{"line":"星汉空如此，宁知心有忆","poemId":"poem_62"},{"line":"星旗映疏勒，云阵上祁连","poemId":"poem_74"},{"line":"星垂平野阔，月涌大江流","poemId":"poem_391"},{"line":"星临万户动，月傍九霄多","poemId":"poem_399"},{"line":"星河秋一雁，砧杵夜千家","poemId":"poem_441"},{"line":"星宫之君醉琼浆，羽人稀少不在旁","poemId":"poem_546"},{"line":"兴来每独往，胜事空自知","poemId":"poem_362"},{"line":"兴尽方下山，何必待之子","poemId":"poem_508"}]