[{"line":"少壮几时兮奈老何","poemId":"poem_14"},{"line":"少壮不努力，老大徒伤悲","poemId":"poem_24"},{"line":"少无适俗韵，性本爱丘山","poemId":"poem_46"},{"line":"少小知名翰墨场，十年心事只凄凉","poemId":"poem_182"},{"line":"少小离家老大回，乡音无改鬓毛衰","poemId":"poem_348"},{"line":"少妇今春意，良人昨夜情","poemId":"poem_407"},{"line":"少孤为客早，多难识君迟","poemId":"poem_431"},{"line":"少小虽非投笔吏，论功还欲请长缨","poemId":"poem_480"},{"line":"少壮能几时","poemId":"poem_505"},{"line":"少陵无人谪仙死，才薄将奈石鼓何","poemId":"poem_551"},{"line":"少年十五二十时，步行夺得胡马骑","poemId":"poem_561"},{"line":"少陵野老吞声哭，春日潜行曲江曲","poemId":"poem_587"},{"line":"少妇城南欲断肠，征人蓟北空回首","poemId":"poem_591"}]