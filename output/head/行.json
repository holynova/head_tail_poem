[{"line":"行行重行行，与君生别离","poemId":"poem_22"},{"line":"行到东溪看水时，坐临孤屿发船迟","poemId":"poem_93"},{"line":"行人日暮少，风雪乱山深","poemId":"poem_127"},{"line":"行行无别语，只道早还乡","poemId":"poem_226"},{"line":"行人便觉须眉绿，一路蝉声过许州","poemId":"poem_249"},{"line":"行到水穷处，坐看云起时","poemId":"poem_362"},{"line":"行当浮桂棹，未几拂荆扉","poemId":"poem_495"},{"line":"行宫见月伤心色，夜雨闻铃肠断声","poemId":"poem_526"},{"line":"行路难","poemId":"poem_566"},{"line":"行路难","poemId":"poem_566"},{"line":"行路难，归去来","poemId":"poem_567"},{"line":"行人刁斗风沙暗，公主琵琶幽怨多","poemId":"poem_584"}]