[{"line":"我戍未定，靡使归聘","poemId":"poem_1"},{"line":"我心伤悲，莫知我哀","poemId":"poem_1"},{"line":"我有嘉宾，鼓瑟吹笙","poemId":"poem_32"},{"line":"我是虏家儿，不解汉儿歌","poemId":"poem_73"},{"line":"我亦且如常日醉，莫教弦管作离声","poemId":"poem_108"},{"line":"我家洗砚池边树，朵朵花开淡墨痕","poemId":"poem_205"},{"line":"我劝天公重抖擞，不拘一格降人才","poemId":"poem_266"},{"line":"我自横刀向天笑，去留肝胆两昆仑","poemId":"poem_273"},{"line":"我行殊未已，何日复归来","poemId":"poem_374"},{"line":"我家襄水曲，遥隔楚云端","poemId":"poem_401"},{"line":"我心素已闲，清川澹如此","poemId":"poem_494"},{"line":"我歌月徘徊，我舞影零乱","poemId":"poem_498"},{"line":"我醉君复乐，陶然共忘机","poemId":"poem_507"},{"line":"我持长瓢坐巴丘，酌饮四坐以散愁","poemId":"poem_528"},{"line":"我闻琵琶已叹息，又闻此语重唧唧","poemId":"poem_529"},{"line":"我从去年辞帝京，谪居卧病浔阳城","poemId":"poem_529"},{"line":"我欲因之梦吴越，一夜飞度镜湖月","poemId":"poem_534"},{"line":"我本楚狂人，凤歌笑孔丘","poemId":"poem_540"},{"line":"我来正逢秋雨节，阴气晦昧无清风","poemId":"poem_553"}]