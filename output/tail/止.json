[{"line":"采薇采薇，薇亦作止","poemId":"poem_1"},{"line":"曰归曰归，岁亦莫止","poemId":"poem_1"},{"line":"采薇采薇，薇亦柔止","poemId":"poem_1"},{"line":"曰归曰归，心亦忧止","poemId":"poem_1"},{"line":"采薇采薇，薇亦刚止","poemId":"poem_1"},{"line":"曰归曰归，岁亦阳止","poemId":"poem_1"},{"line":"相鼠有齿，人而无止","poemId":"poem_9"},{"line":"差池不相见，黾勉空仰止","poemId":"poem_508"}]