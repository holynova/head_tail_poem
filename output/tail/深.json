[{"line":"山不厌高，海不厌深","poemId":"poem_32"},{"line":"行人日暮少，风雪乱山深","poemId":"poem_127"},{"line":"不愁屋漏床床湿，且喜溪流岸岸深","poemId":"poem_149"},{"line":"形容不识识乡音，挑尽寒灯到夜深","poemId":"poem_242"},{"line":"响遏碧云近，香传红藕深","poemId":"poem_260"},{"line":"君问穷通理，渔歌入浦深","poemId":"poem_363"},{"line":"国破山河在，城春草木深","poemId":"poem_395"},{"line":"水落鱼梁浅，天寒梦泽深","poemId":"poem_408"},{"line":"竹径通幽处，禅房花木深","poemId":"poem_418"},{"line":"曲径通幽处，禅房花木深","poemId":"poem_418"},{"line":"竹径通幽处，禅房花木深","poemId":"poem_418"},{"line":"曲径通幽处，禅房花木深","poemId":"poem_418"},{"line":"野寺人来少，云峰水隔深","poemId":"poem_426"},{"line":"西陆蝉声唱，南冠客思深","poemId":"poem_436"},{"line":"长乐钟声花外尽，龙池柳色雨中深","poemId":"poem_488"},{"line":"可以荐嘉客，奈何阻重深","poemId":"poem_525"}]